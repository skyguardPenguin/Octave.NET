<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <!-- The identifier that must be unique within the hosting gallery -->
    <id>Octave.NET</id>

    <!-- The package version number that is used when resolving dependencies -->
    <version>3.0.0</version>


    <description>Use GNU Octave in .NET applications.</description>

    <!-- Authors contain text that appears directly on the gallery -->
    <authors>Michał Wilski</authors>

    <!-- 
            Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  
        -->
    <owners>triforcely</owners>

    <!-- License and project URLs provide links for the gallery -->
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/triforcely/Octave.NET</projectUrl>
    

    <!-- 
            If true, this value prompts the user to accept the license when
            installing the package. 
        -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Any details about this particular release -->
    <releaseNotes>
      Better exception when process can't be spawned.
    </releaseNotes>

    <!-- Copyright information -->
    <copyright>Copyright (c) 2018 Michał Wilski</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>octave matlab data mining analysis</tags>

    <dependencies>
      <dependency id="System.Diagnostics.Process" version="4.3.0" />
      <dependency id="System.Threading.Thread" version="4.3.0" />
    </dependencies>

  </metadata>
  <files>
    <file src="bin\Release\netstandard1.3\Octave.NET.dll" target="lib\netstandard1.3\" />
    <file src="bin\Release\netstandard1.3\Octave.NET.xml" target="lib\netstandard1.3\" />
  </files>
</package>